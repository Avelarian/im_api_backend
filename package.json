{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node index.js",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:generate": "npx sequelize-cli migration:generate",
    "migration:rollback": "npx sequelize-cli db:migrate:undo:all",
    "seeds:run": "npx sequelize-cli db:seed:all",
    "seeds:generate": "npx sequelize-cli seed:generate --name",
    "seeds:rollback": "npx sequelize-cli db:seed:undo:all",
    "lint": "eslint ./"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "mustache": "^4.2.0",
    "nodemailer": "2.1.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "sequelize-cli": "^6.2.0"
  }
}
